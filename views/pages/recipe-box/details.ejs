<!DOCTYPE html>
<html>
  <head>
    <title>TDK:Result Details</title>
    <%- include('../../partials/head.ejs')%>
  </head>
  <body>
    <header class="header">
      <div class="header-content">
        <img class="logo" src="../assets/logos/right/tdk_logo_right_250.png">
        <h1 class="header-title">Searched Recipe</h1>
      </div>
      <div class="nav-bar">
        <%- include('../partials/header.ejs') %>
      </div>
    </header>
    <div class="top-push"></div>
  <div class="content">
      <main>
      <section class="recipe-card">
        <%- include('../../partials/recipe-card-header.ejs') %>
        <div class="recipe-card-body">
          <h3 class="card-title">Ingredients</h3>
          <ul class="ingredients">
            <%= recipe.ingredients %>
          </ul>
          <h3>Instructions</h3>
          <p class="recipe-instructions">
            <ul>

              <%- recipe.instructions %></p>
            </ul>
        </div>
      </section>
      <section class="btn-bar">
        <button class="delete-btn" value="Delete">Delete Me</button>
  
      </section>

    </main>
  </div>
  <div class="push"></div>
  <footer>
    <div class="footer-box">
      <%- include('partials/navigation.ejs') %>
    </div>
  </footer>
    

    <script src="../../js/nav.js"></script>
  </body>
</html>